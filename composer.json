{
    "name": "wyrihaximus/json-psr7",
    "description": "JSON encode and decode PSR-7 requests and responses",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ancarda/psr7-string-stream": "^1.3",
        "cakephp/utility": "^3.7 || ^4.0",
        "laminas/laminas-diactoros": "^2.3",
        "psr/http-message": "^1.0",
        "react/http": "^1.0 || ^0.8.1",
        "ringcentral/psr7": "^1.2.2",
        "thecodingmachine/safe": "^1.1",
        "wyrihaximus/json-utilities": "^1.2"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.6",
        "nyholm/psr7": "^1.2",
        "slim/psr7": "^0.5.0 || ^0.6 || ^1.0",
        "wyrihaximus/test-utilities": "^2.5"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\": "src/"
        },
        "files": [
            "src/functions_include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
